        <steps>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-choose-site-export"><cmd/></step> -->
            <step id="step-choose-site-export">
                <cmd>Choose a site to export from.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ChooseSite-E91.png"><alt>Site dropdown</alt></image></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-choose-site-import"><cmd/></step> -->
            <step id="step-choose-site-import">
                <cmd>Choose a site to import to.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ChooseSite-E91.png"><alt>Site dropdown</alt></image></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-export-data-button"><cmd/></step> -->
            <step id="step-export-data-button">
                <cmd>Click/Tap the <uicontrol>Export Data</uicontrol> button.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ExportData-button-E91.png"><alt>Export Data button</alt></image></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-import-data-button"><cmd/></step> -->
            <step id="step-import-data-button">
                <cmd>Click/Tap the <uicontrol>Import Data</uicontrol> button.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ImportData-button-E91.png"><alt>Import Data button</alt></image></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-configure-export-settings"><cmd/></step> -->
            <step id="step-configure-export-settings">
                <cmd>Configure the export settings.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ExportData-E91.png"><alt>Export settings</alt></image>
                    <simpletable conref="bptext-import-export.dita#tsk-bptext-import-export/simpletable-export-settings"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable>
                </info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-select-package-to-import"><cmd/></step> -->
            <step id="step-select-package-to-import">
                <cmd>Click/Tap the package to import. Then <uicontrol>Continue</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-select-package-to-import-E91.png"><alt>Package selected for import; Continue button enabled.</alt></image></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-configure-import-settings"><cmd/></step> -->
            <step id="step-configure-import-settings">
                <cmd>Configure the import settings.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-ImportData-E91.png"><alt>Import settings</alt></image>
                    <simpletable conref="bptext-import-export.dita#tsk-bptext-import-export/simpletable-import-settings"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable>
                </info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-find-export-package"><cmd/></step> -->
            <step id="step-find-export-package">
                <cmd>In the source server (where the export was done), find the export package(s).</cmd>
                <substeps>
                    <substep>
                        <cmd><ph conkeyref="k-bppbar/ph-pb-host-Settings-Import-Export-E91"></ph></cmd>
                        <info><fig conkeyref="k-bppbar/fig-pb-host-Settings-Import-Export-E91"></fig></info>
                    </substep>
                    <substep>
                        <cmd>Under <uicontrol>Import / Export Log</uicontrol>, search for the export package(s) you want.</cmd>
                        <info><note conref="bptext-import-export.dita#tsk-bptext-import-export/tip-set-filter-to-all-exports"></note></info>
                    </substep>
                    <substep>
                        <cmd>Note the <uicontrol>Folder Name</uicontrol> and the <uicontrol>Export Mode</uicontrol>.</cmd>
                        <info><note conref="bptext-import-export.dita#tsk-bptext-import-export/warning-copy-all-differential-exports"></note></info>
                    </substep>
                    <substep>
                        <cmd>In Windows Explorer, go to the <filepath>App_Data/ExportImport</filepath> folder.</cmd>
                        <info><p>Each subfolder is an export package.</p></info>
                    </substep>
                </substeps>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-find-target-folder"><cmd/></step> -->
            <step id="step-find-target-folder">
                <cmd>In the target server (where the import will be done), go to the <filepath>App_Data/ExportImport</filepath> folder.</cmd>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-copy-export-package"><cmd/></step> -->
            <step id="step-copy-export-package">
                <cmd>Copy the export package subfolder from the source server to the target server.</cmd>
                <info><note conref="bptext-import-export.dita#tsk-bptext-import-export/important-same-path-target-source"></note></info>
            </step>

            <!-- <step conref="bptext-import-export.dita#tsk-bptext-import-export/step-view-target-import-export-log"><cmd/></step> -->
            <step id="step-view-target-import-export-log">
                <cmd>View the <uicontrol>Import / Export Log</uicontrol> of the DNN installation in the target server.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Settings-ImportExport-Log-E91.png"><alt>Import / Export Log</alt></image></info>
            </step>

        </steps>


