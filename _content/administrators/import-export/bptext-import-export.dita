                    <stentry>If enabled (<uicontrol>On</uicontrol>), the process starts immediately.</stentry>
                </strow>
                <!-- <strow conref="bptext-import-export.dita#tsk-bptext-import-export/strow-field-export-mode"><stentry/></strow> -->
                <strow id="strow-field-export-mode">
                    <stentry><uicontrol>Export Mode</uicontrol></stentry>
                    <stentry><ul conref="bptext-import-export.dita#tsk-bptext-import-export/ul-export-modes"><li/></ul></stentry>
                </strow>
                <!-- <strow conref="bptext-import-export.dita#tsk-bptext-import-export/strow-field-overwrite-collisions"><stentry/></strow> -->
                <strow id="strow-field-overwrite-collisions">
                    <stentry><uicontrol>Overwrite Collisions</uicontrol></stentry>
                    <stentry>If enabled (<uicontrol>On</uicontrol>), objects in the target site will be overwritten.</stentry>
                </strow>
            </simpletable>

            <!-- <simpletable conref="bptext-import-export.dita#tsk-bptext-import-export/simpletable-export-settings"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable> -->
