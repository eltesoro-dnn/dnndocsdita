            <!-- <simpletable conref="bptext-import-export.dita#tsk-bptext-import-export/simpletable-import-settings"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable> -->
            <simpletable id="simpletable-import-settings" relcolwidth="1* 3*">
                <sthead>
                    <stentry>Field</stentry>
                    <stentry>Description</stentry>
                </sthead>
                <strow conref="bptext-import-export.dita#tsk-bptext-import-export/strow-field-overwrite-collisions"><stentry/></strow>
                <strow conref="bptext-import-export.dita#tsk-bptext-import-export/strow-field-run-now"><stentry/></strow>
            </simpletable>

