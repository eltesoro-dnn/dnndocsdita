    <refbody>

        <section>

            <sectiondiv id="sectiondiv-bpdesc">
				<!-- <ph conref="module-blogs.dita#ref-ModuleBlogs/ph-bpshortdesc"></ph> -->
				<p><ph id="ph-bpshortdesc">Enables users to publish single- or multi-author blogs. Allows content to be authored using third-party tools, such as Windows Live Writer.</ph></p>
            </sectiondiv>


            <!-- <sectiondiv conref="module-blogs.dita#ref-ModuleBlogs/sectiondiv-bpimg"></sectiondiv> -->
            <sectiondiv id="sectiondiv-bpimg">
                <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-module-Blogs.png"><alt>Blogs module</alt></image>
            </sectiondiv>


            <!-- <sectiondiv conref=".../sectiondiv-bpsecurity"></sectiondiv>
