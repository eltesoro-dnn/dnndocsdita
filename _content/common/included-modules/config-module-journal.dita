    <title>Configure the Journal Module</title>
    <titlealts>
        <navtitle>Configure Journal</navtitle>
    </titlealts>


    <prolog>
        <author>wrET,orLY,edET,1605</author>
        <copyright>
            <copyryear year="2016"></copyryear>
