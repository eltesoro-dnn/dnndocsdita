        <section>

            <sectiondiv id="sectiondiv-bpdesc">
				<!-- <ph conref="module-publisher.dita#ref-module-publisher/ph-bpshortdesc"></ph> -->
				<p><ph id="ph-bpshortdesc">Enables content managers to create articles, and allows users to view those articles.</ph></p>
            </sectiondiv>


            <!-- <sectiondiv conref="module-publisher.dita#ref-module-publisher/sectiondiv-bpimg"></sectiondiv> -->
