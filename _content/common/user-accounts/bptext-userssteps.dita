                <choices>
                    <choice>
                        <p>To edit the user's profile: <ph conref="#tsk-bptext-userssteps/ph-user-profilesettings"></ph></p>
                        <image conref="bptext-userssteps.dita#tsk-bptext-userssteps/image-user-profilesettings"></image>
                    </choice>
                    <choice>
                        <p>To edit the user's account: <ph conref="#tsk-bptext-userssteps/ph-user-accountsettings"></ph></p>
                        <image conref="bptext-userssteps.dita#tsk-bptext-userssteps/image-user-accountsettings"></image>
                    </choice>
                </choices>
            </step>


