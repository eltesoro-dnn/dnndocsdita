                <cmd><ph id="ph-user-accountsettings">Click/Tap the <uicontrol>Account Settings</uicontrol> (gear) icon for the user.</ph></cmd>
                <info><image id="image-user-accountsettings" outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-UserList-AccountSettings-E90.png"><alt>User List &gt; find the user &gt; Account Settings</alt></image></info>
            </step>

            <!-- <step conref="bptext-userssteps.dita#tsk-bptext-userssteps/step-user-edit"><cmd/></step> -->
