<task xml:lang="en" id="tsk-submit-site-Google-search">

    <title>Submit Your Site to Google Search for Indexing</title>
    <titlealts>
        <navtitle>Submit Site to Google Search</navtitle>
    </titlealts>


    <prolog>
        <author>wrET,orLY,edET,1606</author>
        <copyright>
            <copyryear year="2016"></copyryear>
            <copyrholder>DNN Corp</copyrholder>
        </copyright>
        <metadata>
            <audience type="administrator" job="other" otherjob="configuring" />
            <audience type="other" othertype="content manager" job="other" otherjob="configuring" />
            <prodinfo>
                <prodname>Platform</prodname>
                <vrmlist><vrm version="8" release="0"/></vrmlist>
            </prodinfo>
            <prodinfo>
                <prodname>Evoq Content</prodname>
                <vrmlist><vrm version="8" release="3"/></vrmlist>
