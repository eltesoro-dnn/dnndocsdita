        <steps>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-createbtn"><cmd/></step> -->
            <step id="step-contenttype-createbtn">
                <cmd>Click/Tap <uicontrol>Create Content Type</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-pbtabs-all-Content-ContentLibrary-ContentTypes-CreateContentTypeButton-E91.png"><alt>Content &gt; Content Types tab &gt; Create Content Type</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-highlightellipses-edit"><cmd/></step> -->
            <step id="step-contenttype-highlightellipses-edit">
                <cmd>Highlight the content type, click/tap the ellipses, and choose <uicontrol>Edit</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentTypes-Ellipses-Edit-E91.png"><alt>Content Type ellipses menu &gt; Edit</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-highlightellipses-duplicate"><cmd/></step> -->
            <step id="step-contenttype-highlightellipses-duplicate">
                <cmd>Highlight the content type, click/tap the ellipses, and choose <uicontrol>Duplicate</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentTypes-Ellipses-Duplicate-E91.png"><alt>Content Type ellipses menu &gt; Duplicate</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-highlightellipses-delete"><cmd/></step> -->
            <step id="step-contenttype-highlightellipses-delete">
                <cmd>Highlight the content type, click/tap the ellipses, and choose <uicontrol>Delete</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentTypes-Ellipses-Delete-E91.png"><alt>Content Type ellipses menu &gt; Delete</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-configurecontenttype"><cmd/></step> -->
            <step id="step-contenttype-configurecontenttype">
                <cmd>Configure the content type.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentTypes-Settings.png"><alt>Content Type settings</alt></image>
                    <simpletable conref="bptext-content.dita#tsk-bptext-content/simpletable-contenttype-configurecontenttype"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable>
                </info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-configurefields"><cmd/></step> -->
            <step id="step-contenttype-configurefields">
                <cmd>Manage the fields of the content type.</cmd>
                <substeps>
                    <substep>
                        <cmd><ph conkeyref="k-bptext/ph-fieldadd"></ph></cmd>
                        <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentField-Add.png"><alt>Drag and drop a field to the canvas.</alt></image></info>
                    </substep>
                    <substep>
                        <cmd><ph conkeyref="k-bptext/ph-fieldedit"></ph></cmd>
                        <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentField-Edit.png"><alt>Click/Tap a field in the canvas, then configure its settings.</alt></image></info>
                    </substep>
                    <substep>
                        <cmd><ph conkeyref="k-bptext/ph-fielddup"></ph></cmd>
                        <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentField-Hover-Dup.png"><alt>Hover over a field in the canvas, then click/tap the duplicate icon.</alt></image></info>
                    </substep>
                    <substep>
                        <cmd><ph conkeyref="k-bptext/ph-fielddel"></ph></cmd>
                        <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentField-Hover-Del.png"><alt>Hover over a field in the canvas, then click/tap the trash icon.</alt></image></info>
                    </substep>
                </substeps>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-contenttype-configureproperties"><cmd/></step> -->
            <step id="step-contenttype-configureproperties">
                <cmd>Configure the content type's properties.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-ContentTypes-Properties.png"><alt>Content Type properties</alt></image>
                    <simpletable conref="bptext-content.dita#tsk-bptext-content/simpletable-contenttype-configureproperties"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable>
                </info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-visualizer-createbtn-new"><cmd/></step> -->
            <step id="step-visualizer-createbtn-new">
                <cmd>Click/Tap <uicontrol>Create Visualizer</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Visualizers-CreateBtn-New-E91.png"><alt>Content Library &gt; Visualizers tab &gt; Create Visualizer &gt; New Visualizer</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-visualizer-createbtn-import"><cmd/></step> -->
            <step id="step-visualizer-createbtn-import">
                <cmd>Click/Tap <uicontrol>Create Visualizer</uicontrol>.</cmd>
                <info><image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Visualizers-CreateBtn-Import-E91.png"><alt>Content Library &gt; Visualizers tab &gt; Create Visualizer &gt; Import Visualizer</alt></image></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-visualizer-choose-zip-import"><cmd/></step> -->
            <step id="step-visualizer-choose-zip-import">
                <cmd>Choose the visualizer zip file to import.</cmd>
                <info><p>The imported zip file must be a valid visualizer package. If the visualizer already exists, choose whether to override it or create a new copy.</p></info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-visualizer-choosevisualizer"><cmd/></step> -->
            <step id="step-visualizer-choosevisualizer">
                <cmd>Search for the visualizer you want, then click/tap on its icon.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Visualizers-List-SearchClickTap-E91.gif"><alt>Visualizers list - Click/Tap the visualizer you want.</alt></image>
                    <p>You can browse the list of visualizers, or you can use <uicontrol>Search</uicontrol> field.</p>
                </info>
            </step>

            <!-- <step conref="bptext-content.dita#tsk-bptext-content/step-visualizer-configurevisualizerproperties"><cmd/></step> -->
            <step id="step-visualizer-configurevisualizerproperties">
                <cmd>Configure the properties of the visualizer.</cmd>
                <info>
                    <image outputclass="img-scr" scalefit="yes" placement="break" align="left" href="../../common/img/scr-Visualizers-Details-E91.png"><alt>Visualizer Details tab</alt></image>
                    <simpletable conref="bptext-content.dita#tsk-bptext-content/simpletable-visualizer-configurevisualizerproperties"><sthead><stentry/></sthead><strow><stentry/></strow></simpletable>
