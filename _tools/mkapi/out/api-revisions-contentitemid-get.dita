<!DOCTYPE topic PUBLIC "-//OASIS//DTD DITA Topic//EN" "http://docs.oasis-open.org/dita/v1.2/os/dtd1.2/technicalContent/dtd/topic.dtd">
<topic xml:lang="en" id="top-api-revisions-contentitemid-get">


    <title>GET Revisions</title>
    <titlealts>
        <navtitle>GET Revisions</navtitle>
    </titlealts>

    <body>

        <section id="apidesc">
            <title>GET /api/Revisions/{contentItemId}</title>
            <p></p>
        </section>


        <section id="http-request">
            <title>HTTP Request</title>

            <sectiondiv id="sectiondiv-url">

                <p outputclass="sectiondivtitle docindent">In the URL,</p>

                <codeblock outputclass="syntaxblock docindent">
<ph conkeyref="k-bpapi/ph-apibaseurl"></ph>/api/Revisions/<varname>{contentItemId}</varname>?<varname>query</varname>
                </codeblock>

                <p outputclass="paramlisttitle docindent">Path Parameters</p>
                <parml outputclass="docindent">
                    <plentry>
                        <pt>contentItemId</pt>
                        <pd>[string] </pd>
                    </plentry>
                </parml>

                <p outputclass="paramlisttitle docindent">Query Fields</p>
                <parml outputclass="docindent">
                    <plentry>
                        <pt>searchtext</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>startindex</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>maxitems</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>fieldorder</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>orderasc</pt>
                        <pd>[boolean] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>createdfrom</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                    <plentry>
                        <pt>createdto</pt>
                        <pd>[string] (Optional) </pd>
                    </plentry>
                </parml>
            </sectiondiv>
        </section>


        <section id="http-response-body">
            <title>HTTP Response</title>

            <p>Status: <systemoutput>200</systemoutput></p>

            <codeblock outputclass="syntaxblock docindent">
{
    [
    <ph conkeyref="k-bpapi/ph-definitions-StructuredContent-API-Dto-ContentItems-ContentItemRevision"></ph>
        , ...
    ]
}
            </codeblock>

            <p outputclass="paramlisttitle docindent">Response Schema Values</p>
            <parml outputclass="docindent">
                <plentry>
                    <pt>type</pt>
                    <pd></pd>
                </plentry>
                <plentry>
                    <pt>items</pt>
                    <pd>
                        <parml conkeyref="k-bpapi/parml-definitions-StructuredContent-API-Dto-ContentItems-ContentItemRevision"><plentry><pt/><pd/></plentry></parml>
                    </pd>
                </plentry>
            </parml>
        </section>

    </body>

